# Environment variables for production deployment
# Copy this file to .env and fill in your actual values

# MySQL Database
MYSQL_ROOT_PASSWORD=your_secure_password_here

# Backend Service URLs (for external access)
BACKEND_URL=http://your-vps-ip:8080
SSO_URL=http://your-vps-ip:8081
DATACORE_URL=http://your-vps-ip:8082
LIBRARY_URL=http://your-vps-ip:8083

# Frontend URL (after deploying to Vercel/Netlify)
FRONTEND_URL=https://your-app.vercel.app

# Optional: Domain name if you have one
# DOMAIN=yourdomain.com
